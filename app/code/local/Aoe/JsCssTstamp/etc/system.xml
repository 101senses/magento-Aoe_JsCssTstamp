<?xml version="1.0" ?>
<config>
	<sections>

		<dev translate="label" module="apilog">
			<groups>
				<js>
					<fields>
						<minify_files translate="label">
							<label>Minify JavaScript Files</label>
							<comment><![CDATA[Only available if merging is enabled]]></comment>
							<frontend_type>select</frontend_type>
							<source_model>adminhtml/system_config_source_yesno</source_model>
							<sort_order>20</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</minify_files>
						<!--<compress_files translate="label">
							<label>Compress JavaScript Files</label>
							<comment><![CDATA[Only available if merging is enabled]]></comment>
							<frontend_type>select</frontend_type>
							<source_model>adminhtml/system_config_source_yesno</source_model>
							<sort_order>30</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</compress_files>-->
						<lifetime translate="label">
							<label>Lifetime</label>
							<comment><![CDATA[Expires and Cache-Control header lifetimes to be set while uploading to Amazon S3 (will only work with OnePica_ImageCDN enabled)]]></comment>
							<frontend_type>text</frontend_type>
							<sort_order>40</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</lifetime>
						<debug translate="label">
							<label>Write debug to system.log</label>
							<frontend_type>select</frontend_type>
							<source_model>adminhtml/system_config_source_yesno</source_model>
							<sort_order>50</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</debug>
					</fields>
				</js>
				<css>
					<fields>
						<lifetime translate="label">
							<label>Lifetime</label>
							<comment><![CDATA[Expires and Cache-Control header lifetimes to be set while uploading to Amazon S3 (will only work with OnePica_ImageCDN enabled)]]></comment>
							<frontend_type>text</frontend_type>
							<sort_order>20</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</lifetime>
					</fields>
				</css>
			</groups>
		</dev>
	</sections>
</config>
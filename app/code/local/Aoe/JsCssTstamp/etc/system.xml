<?xml version="1.0" ?>
<config>
    <sections>
        <dev>
            <groups>
                <js>
                    <fields>
                        <protocolRelativeUris translate="label comment" module="aoejscsststamp">
                            <label>Protocol relative uris</label>
                            <comment><![CDATA[e.g. http://example.com -> //example.com]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </protocolRelativeUris>
                        <storeInDb translate="label comment" module="aoejscsststamp">
                            <label>Store files in database</label>
                            <comment><![CDATA[Needed on a multi-server setup. Check readme.txt file for additional rewrite rule!]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </storeInDb>
                        <storeMinifiedJsFolder translate="label comment" module="aoejscsststamp">
                            <label>Minified js files folder</label>
                            <comment><![CDATA[Folder to additionally look for minified versions of js files, for example minified/js]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </storeMinifiedJsFolder>
                    </fields>
                </js>
                <css>
                    <fields>
                        <protocolRelativeUris translate="label comment" module="aoejscsststamp">
                            <label>Protocol relative uris</label>
                            <comment><![CDATA[e.g. http://example.com -> //example.com]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </protocolRelativeUris>
                        <storeInDb translate="label comment" module="aoejscsststamp">
                            <label>Store files in database</label>
                            <comment><![CDATA[Needed on a multi-server setup. Check readme.txt file for additional rewrite rule!]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </storeInDb>
                        <addTstampToAssets translate="label comment" module="aoejscsststamp">
                            <label>Append timestamp to png/css/jpg files</label>
                            <comment><![CDATA[Check readme.txt for additional rewrite rule!]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </addTstampToAssets>
                        <storeMinifiedCssFolder translate="label comment" module="aoejscsststamp">
                            <label>Minified css files folder</label>
                            <comment><![CDATA[Folder to additionally look for minified versions of css files, for example minified/css]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>50</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </storeMinifiedCssFolder>
                    </fields>
                </css>
            </groups>
        </dev>
    </sections>
</config>
